CREATE TABLE IF NOT EXISTS random_table (i INT, ni INT NULL, f FLOAT, b BOOLEAN, s VARCHAR, d DATE, t TIMESTAMP, a ARRAY(INT)) ENGINE=RANDOM;

statement query I
SELECT COUNT(*) FROM (SELECT * FROM random_table LIMIT 5);

----
5

